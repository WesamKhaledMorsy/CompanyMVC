@model string
<td>
      <a asp-action="Details" asp-route-id="@Model" class="btn btn-outline-info">
          <i class="fa-solid fa-file-lines"></i>
      </a>
</td>
<td>
      <a asp-action="Update" asp-route-id="@Model" class="btn btn-outline-success">
          <i class="fa-solid fa-edit"></i>
      </a>

</td>
<td>
      <a asp-action="Delete" asp-route-id="@Model" class="btn btn-outline-danger">
          <i class="fa-solid fa-circle-xmark"></i>
      </a>
</td>   